{
    "name": "circuitorio",
    "displayName": "Circuitorio",
    "description": "Syntax highlighting for circuitorio",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "circuitorio",
                "aliases": [
                    "Circuitorio"
                ],
                "extensions": [
                    "fhdl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "circuitorio",
                "scopeName": "source.circuitorio",
                "path": "./circuitorio.tmLanguage.json"
            }
        ]
    }
}